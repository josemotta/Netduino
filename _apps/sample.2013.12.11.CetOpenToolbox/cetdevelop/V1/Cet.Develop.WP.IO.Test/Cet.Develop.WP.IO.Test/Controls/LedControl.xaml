<UserControl x:Class="Cet.Develop.Windows.IO.LedControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="48" d:DesignWidth="48">
    
    <Grid x:Name="LayoutRoot">
        <Image
            Source="/NetduinoModubusTest;component/Images/green-off-48.png"
            x:Name="ImgOff"
            />
        <Image
            Source="/NetduinoModubusTest;component/Images/green-on-48.png"
            x:Name="ImgOn"
            Visibility="Collapsed"
            />
    </Grid>
</UserControl>
